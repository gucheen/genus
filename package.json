{
  "name": "genus",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Genus",
    "manifest": "src/manifest.json",
    "main": "Genus.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "repository": "https://github.com/keplerlab/genus.git",
  "description": "A sketch plugin for exporting prototype website (static html css) from your sketch designs",
  "license": "MIT",
  "author": "Kepler Lab <keplerwaasi@gmail.com>"
}
